    <DTS:Executable
      DTS:refId="Package\Actualizar Provision de Cobranza"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Tarea Ejecutar SQL"
      DTS:DTSID="{3027FAE4-2392-4F4C-95D7-3A270628F0AD}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Actualizar Provision de Cobranza"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="SqlStatementSource">"Declare @idMotivoModificaDB int = ?, @idEmpresa SmallInt = ? 

IF " + (DT_STR, 1, 1252 ) @[User::SoloTributaria] + " = 0 
BEGIN
	Update pd set ExcluidoProvision = 1 , idMotivoModificaDB = @idMotivoModificaDB
	From cont.Provision (nolock) pp 
	Join cont.ProvisionDocumentos (nolock) pd 
				on pp.IdProvision = pd.IdProvision 
	Where pp.idempresa = @idEmpresa and pp.IdProvisionTipo = 1 and pp.PeriodoContable = (Select top 1 PeriodoReporte From Temporal.paso)

	Update d set ExcluidoProvision = 0, idmotivomodificaDB =  @idMotivoModificaDB
	From Temporal.paso p 
	Join cont.ProvisionDocumentos (nolock) d 
		   on p.suministro = d.idnroservicio 
	Join cont.Provision (nolock) x 
		   on x.idprovision = d.idprovision 
	Where d.idempresa = @idEmpresa And d.idtipodocumento = p.IdTipoDocumento and p.NroDocumento = d.NroDocumento and x.PeriodoContable = p.PeriodoReporte 

	Update dd Set  MontoProvisionAutoriza = tt.ImporteFinal
		   ,  MontoProvisionPreAutoriza = tt.ImporteFinal
		   ,  MontoProvisionpropuesta = tt.ImporteFinal
		   ,  IdMotivoModificaDB =  @idMotivoModificaDB
	From ( Select pp.IdProvision, sum(pd.MontoProvision) ImporteFinal
		   From cont.Provision (nolock) pp
		   inner join cont.ProvisionDocumentos (nolock) pd
				on pp.IdProvision = pd.IdProvision
			Where pp.idempresa = @idEmpresa and pp.PeriodoContable = (Select top 1 PeriodoReporte From Temporal.paso)
			and pp.IdProvisionTipo = 1 and pd.ExcluidoProvision = 0 
			Group by pp.IdProvision ) tt
		   inner join cont.Provision (nolock) dd
				 on dd.IdProvision = tt.IdProvision
END 

Update dd Set  MontoProvisionAutoriza = tt.ImporteFinal
	,  MontoProvisionPreAutoriza = tt.ImporteFinal 
	,  MontoProvisionpropuesta = tt.ImporteFinal 
	, IdMotivoModificaDB =  @idMotivoModificaDB 
From ( Select pp.IdProvision, sum(pd.MontoProvision) ImporteFinal
		From cont.Provision (nolock) pp
		inner join cont.ProvisionDocumentos (nolock) pd
			on pp.IdProvision = pd.IdProvision
		Where pp.idempresa = @idEmpresa and pp.PeriodoContable = (Select top 1 PeriodoReporte From Temporal.paso)
		and pp.IdProvisionTipo = 2 and pd.ExcluidoProvision = 0
		Group by pp.IdProvision ) tt
		inner join cont.Provision (nolock) dd
			on dd.IdProvision = tt.IdProvision
"</DTS:PropertyExpression>
