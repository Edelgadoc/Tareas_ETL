    <DTS:Executable
      DTS:refId="Package\Get Motivo Modifica"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Tarea Ejecutar SQL"
      DTS:DTSID="{CB9FF1A6-54B9-431A-B66B-C5FA118BC40E}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Get Motivo Modifica"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{8E976E38-295A-4941-BFC4-AD7B8FFE760D}"
          SQLTask:SqlStatementSource="If Exists ( Select IdMotivoModificaDB  &#xA;&#x9;&#x9;&#x9;From MotivoModificaDB &#xA;&#x9;&#x9;&#x9;Where Descripcion = 'Marcar documentos provicionados' And TablasAfectadas = 'ProvisionDocumentos' And idestado = 5 )&#xA;&#xA;&#x9;&#x9;&#x9;Select Top 1 IdMotivoModificaDB  &#xA;&#x9;&#x9;&#x9;From MotivoModificaDB &#xA;&#x9;&#x9;&#x9;Where Descripcion = 'Marcar documentos provicionados' And TablasAfectadas = 'ProvisionDocumentos' And idestado = 5&#xA;Else&#xA;Begin &#xA;&#x9;Insert Into MotivoModificaDB (Descripcion, TablasAfectadas, Fecha, IdUsuarioModifica, IdUsuarioAutoriza, IdEstado, IdMesaAyuda)&#xA;&#x9;Select  'Marcar documentos provicionados','ProvisionDocumentos', Getdate(), 20754, 20754, 5, Null &#xA;&#x9;Select @@IDENTITY As IdMotivoModificaDB&#xA;End "
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
            SQLTask:ResultName="IdMotivoModificaDB"
            SQLTask:DtsVariableName="User::IdMotivoModificaDB" />
        </SQLTask:SqlTaskData>
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Obtener Mensaje del Correo"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Tarea Ejecutar SQL"
      DTS:DTSID="{FC56288D-03AE-4E29-A35A-1C120C55ED28}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Obtener Mensaje del Correo"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{8E976E38-295A-4941-BFC4-AD7B8FFE760D}"
          SQLTask:SqlStatementSource="Set Nocount On &#xA;&#xA;Declare @Periodo int, @idEmpresa smallint = ?,  &#xA;@Mensaje varchar(800) = 'Estimados Srs.' + char(13) + char(10) + char(13) + 'Se adjunta la información de la Provision NIF : ' + char(13) + char(10)  &#xA;&#xA;Select @Periodo = PeriodoReporte From Temporal.paso &#xA;&#xA;Drop Table If Exists #Provision&#xA;Create Table #Provision (idProvision int, UnidadNegocio varchar(70), FechaProvision datetime, MontoProvision Money)&#xA;&#xA;Insert Into #Provision &#xA;Exec dbo.provision_asientoresumen_pa @P_IdEmpresa = @idEmpresa, @P_Periodo = @Periodo, @P_IdProvisionTipo = 1&#xA;&#xA;Select @Mensaje = @Mensaje + char(9) + UnidadNegocio + ' : ' + Convert( varchar(20), MontoProvision,1) + char(13) + char(10) From #Provision &#xA;&#xA;Delete From #Provision&#xA;&#xA;Select @Mensaje  = @Mensaje +  char(13) + char(10) + 'Se adjunta la información de la Provision Tributaria : ' + char(13) + char(10)   &#xA;&#xA;Insert Into #Provision &#xA;Exec dbo.provision_asientoresumen_pa @P_IdEmpresa = @idEmpresa, @P_Periodo = @Periodo, @P_IdProvisionTipo = 2&#xA;&#xA;Select @Mensaje = @Mensaje + char(9) + UnidadNegocio + ' : ' + Convert( varchar(20), MontoProvision,1) + char(13) + char(10) From #Provision &#xA;&#xA;Select @Mensaje +  char(13) + char(10) + 'Atte.' As NotificacionBody&#xA;"
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
            SQLTask:ResultName="NotificacionBody"
            SQLTask:DtsVariableName="User::NotificacionBody" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="0"
            SQLTask:DtsVariableName="User::idEmpresa"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="2"
            SQLTask:ParameterSize="-1" />
        </SQLTask:SqlTaskData>
      </DTS:ObjectData>
    </DTS:Executable>
